<RuleML xmlns ="http://www.ruleml.org/0.9/xsd" xsi:schemaLocation="http://www.ruleml.org/0.9/xsd ruleml-0_9-nafdatalog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <Assert>
    <Implies>
      <oid>
        <Ind>label:ANTI-INFECTIVES,OTHER  with INJ are formulary</Ind>
      </oid>
      <body>
        <And>
          <Atom>
            <op>
              <Rel>VaClass</Rel>
            </op>
            <Var>Transaction</Var>
            <Var>Drug</Var>
            <Ind>ANTI-INFECTIVES,OTHER</Ind>
          </Atom>
          <Atom>
            <op>
              <Rel>DosageForm</Rel>
            </op>
            <Var>Transaction</Var>
            <Var>Drug</Var>
            <Ind>INJ</Ind>
          </Atom>
        </And>
      </body>
      <head>
        <Atom>
          <op>
            <Rel>Formulary</Rel>
          </op>
          <Var>Transaction</Var>
          <Ind>True</Ind>
        </Atom>
      </head>
    </Implies>
  </Assert>
</RuleML>